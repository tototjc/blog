{
  "name": "astro-blog",
  "type": "module",
  "version": "0.0.6",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "check": "astro check",
    "lint": "pnpm run \"/^lint:.*/\"",
    "lint:es": "eslint",
    "lint:ts": "tsc --noEmit",
    "lint:css": "stylelint \"**/*.{css,astro}\"",
    "lint:prettier": "prettier . --check",
    "fmt": "pnpm run \"/^fmt:.*/\"",
    "fmt:es": "eslint --fix",
    "fmt:css": "stylelint --fix \"**/*.{css,astro}\"",
    "fmt:prettier": "prettier . --write",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "browserslist": {
    "production": [
      "cover 95% and fully supports es6",
      "not dead"
    ],
    "development": [
      "last 1 version and fully supports es6",
      "not dead"
    ]
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@astrojs/ts-plugin": "^1.10.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.2.9",
    "@pagefind/modular-ui": "^1.3.0",
    "astro": "^5.8.2",
    "decap-cms": "^3.6.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "mrmime": "^2.0.1",
    "pagefind": "^1.3.0",
    "postcss-html": "^1.8.0",
    "postcss-preset-env": "^10.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "reading-time-estimator": "^1.14.0",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "sharp": "^0.34.2",
    "stylelint": "^16.20.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite-plugin-devtools-json": "^0.1.0"
  },
  "packageManager": "pnpm@10.11.1"
}
