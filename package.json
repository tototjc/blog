{
  "name": "astro-blog",
  "type": "module",
  "version": "0.0.3",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "check": "astro check",
    "lint": "pnpm run \"/^lint:.*/\"",
    "lint:es": "eslint",
    "lint:ts": "tsc --noEmit",
    "lint:css": "stylelint \"**/*.{css,astro}\"",
    "lint:prettier": "prettier . --check",
    "fmt": "pnpm run \"/^fmt:.*/\"",
    "fmt:es": "eslint --fix",
    "fmt:css": "stylelint --fix \"**/*.{css,astro}\"",
    "fmt:prettier": "prettier . --write",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "browserslist": {
    "production": [
      "fully supports es6-module",
      "not dead"
    ],
    "development": [
      "last 1 versions"
    ],
    "ssr": [
      "maintained node versions"
    ]
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.2.1",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/compat": "^1.2.5",
    "astro": "^5.1.7",
    "decap-cms": "^3.4.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-astro": "^1.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "postcss-html": "^1.8.0",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "reading-time": "2.0.0-1",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "stylelint": "^16.13.2",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-prettier": "^5.0.2",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "packageManager": "pnpm@9.15.4"
}
